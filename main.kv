			halign: "left"
			theme_text_color: 'Custom'
			text_color: rgba('#707070')
			text_size: (0.4, 0.2)
            pos_hint: {'center_x': 1.015, 'center_y' : 0.775}
		MDTextFieldRound:
			id: lname
			hint_text: 'Enter your last name'
			text_color: rgba('#C69C6D')
			size_hint: (0.3, None)
			pos_hint: {'center_x' : 0.675, 'center_y' : 0.7}
			normal_color: rgba('#f4ebe2')
			fill_color: rgba('#f4ebe2')
			color_active: rgba('#f4ebe2')
			color_focus: rgba('#f4ebe2')
			line_color: rgba('#C69C6D')
			icon_right: "account"
			icon_right_color: rgba('#c69c6d')
		MDLabel:
			text: 'Email Address'
            bold: True
			halign: "left"
			theme_text_color: 'Custom'
			text_color: rgba('#707070')
			text_size: (0.4, 0.2)
            pos_hint: {'center_x': 0.665, 'center_y' : 0.575}
		MDTextFieldRound:
			id: emailid
			hint_text: 'Enter your email address'
			text_color: rgba('#C69C6D')
			size_hint: (0.65, None)
			pos_hint: {'center_x' : 0.5, 'center_y' : 0.505}
			normal_color: rgba('#f4ebe2')
			fill_color: rgba('#f4ebe2')
			color_active: rgba('#f4ebe2')
			color_focus: rgba('#f4ebe2')
			line_color: rgba('#C69C6D')
			icon_right: "email"
			icon_right_color: rgba('#c69c6d')
		MDLabel:
			text: 'Password'
            bold: True
			halign: "left"
			theme_text_color: 'Custom'
			text_color: rgba('#707070')
			text_size: (0.4, 0.2)
            pos_hint: {'center_x': 0.665, 'center_y' : 0.375}
		MDTextFieldRound:
			id: passwd
			hint_text: 'Enter your desired password'
			text_color: rgba('#C69C6D')
			size_hint: (0.3, None)
			pos_hint: {'center_x' : 0.325, 'center_y' : 0.305}
			normal_color: rgba('#f4ebe2')
			fill_color: rgba('#f4ebe2')
			color_active: rgba('#f4ebe2')
			color_focus: rgba('#f4ebe2')
			line_color: rgba('#C69C6D')
			icon_right: "key-variant"
			icon_right_color: rgba('#c69c6d')
		MDLabel:
