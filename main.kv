		canvas:
			Color:
				rgba: rgba('#C69C6D')
			Rectangle:
				size: self.size
				pos: self.pos
	MDIconButton:
		icon : 'keyboard-backspace'
		pos_hint: {'center_x': 0.05, 'center_y' : 0.94}
		theme_text_color : 'Custom'
		text_color : (1, 1, 1, 1)
