<HomeScreen>:
	name : 'home_screen'

	Widget:
		canvas:
			Color:
				rgba: rgba('#C69C6D')
			Rectangle:
				size: self.size
				pos: self.pos

	MDBoxLayout:
		orientation: "vertical"

		MDBoxLayout:
			MDNavigationRail:
				id: rail
				md_bg_color: rgba('#f4ebe2')
				color_normal: rgba("#707070")
				color_active: rgba("#C69C6D")
				visible: "Persistent"

				MDNavigationRailItem:
					icon: "home"
					on_release:
						screen_manager.current = 'home'

				MDNavigationRailItem:
					icon: "folder-open"
					on_release:
						screen_manager.current = 'add_section'

				MDNavigationRailItem:
					icon: "logout"
					on_press:
						root.manager.transition.direction = 'right'
						root.manager.transition.duration = .3
						root.manager.current = 'login_screen'

			ScreenManager:
				id: screen_manager

				Screen:
					name: 'home'

					#Contents of Gray box

					FloatLayout: #White box left
						pos_hint: {'center_y': 0.5, 'center_x': 0.372}
						size_hint: 0.70, 0.85
						canvas:
							Color:
								rgba: rgba('#FFFFFF')
							RoundedRectangle:
								size: self.size
								pos: self.pos
								radius: [70, 0, 0, 70]

					FloatLayout: #Gray box left
						pos_hint: {'center_y': 0.5, 'center_x': 0.122}
						size_hint: 0.20, 0.85
						canvas:
							Color:
								rgba: rgba('#c7c7c7')
							RoundedRectangle:
								size: self.size
								pos: self.pos
								radius: [70, 0, 0, 70]

					BoxLayout:
						pos_hint: {'center_y': 0.640, 'center_x': 0.125}
						size_hint: 0.16, 0.3

						ScrollView:
							MDList:
								id: container
								index: 0

					MDLabel:
						text: '[font=Fonts/segoeuib] HASHTAGS [/font]'
						markup: True
						halign: "left"
						theme_text_color:'Custom'
						text_color: rgba('#FFFFFF')
						font_size: '18sp'
						pos_hint: {'center_x': 0.539, 'center_y' : 0.83}

					MDIconButton: #Plus icon
						icon: 'plus-circle'
						pos_hint: {'center_x': 0.197, 'center_y': 0.83}
						user_font_size: '18sp'
						theme_text_color: "Custom"
						text_color: rgba('#FFFFFF')
						on_release: root.add_hashtag_dialog()

