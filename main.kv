			md_bg_color: rgba('#C69C6D')
			pos_hint: {'center_x' : 0.5, 'center_y' : 0.155}
			on_press:
				root.do_register(fname.text, lname.text, emailid.text, passwd.text, cpass.text)

<ForgotScreen>:
	name : 'forgot_screen'
	Widget:
		canvas:
			Color:
				rgba: rgba('#C69C6D')
			Rectangle:
				size: self.size
				pos: self.pos

<AddHashtag>
    orientation: "vertical"
    spacing: "12dp"
    size_hint_y: None
    height: "80dp"

    MDTextField:
		id: task_hashtag
        hint_text: "Hashtag"
		line_color_focus: rgba('#C6826D')

	MDFillRoundFlatButton:
		text: "Add"
		theme_text_color: 'Custom'
		md_bg_color: rgba('#C6826D')
		text_color: utils.get_color_from_hex('#F1ECEC')
		pos_hint: {'center_x': .93, 'center_y': .5}
		on_release:
			root.add_hashtag(task_hashtag.text)

<Check@MDCheckbox>:
    group: 'group'
    size_hint: None, None
    size: dp(48), dp(48)
	theme_text_color: "Custom"
	selected_color: rgba('#C6826D')

<AddTask>
    orientation: "vertical"
    spacing: "12dp"
    size_hint_y: None
    height: "285dp"

	task_name: task_name
	task_details: task_details

    FloatLayout:
		MDLabel:
			text: 'Important'
			pos_hint: {'center_x': .7, 'center_y': .5}
		Check:
			id: importantc
			active: True
			on_active: root.on_checkbox_active(*args)
			pos_hint: {'center_x': .4, 'center_y': .5}
		MDLabel:
			text: 'Normal'
			pos_hint: {'center_x': 1.17, 'center_y': .5}
		Check:
			id: normalc
			active: False
			pos_hint: {'center_x': .6, 'center_y': .5}

    MDTextField:
		id: task_name
        hint_text: "Task Name"
		line_color_focus: rgba('#C6826D')

    MDTextField:
		id: task_details
        hint_text: "Task Details"
		line_color_focus: rgba('#C6826D')

    MDRaisedButton:
        text: "Set Task Due Date"
		theme_text_color: "Custom"
		md_bg_color: rgba('#C6826D')
		text_color: utils.get_color_from_hex('#F1ECEC')
        on_release: root.show_date_picker()
		pos_hint: {'center_x': .5, 'center_y': .5}

    MDRaisedButton:
        text: "Set Task Due Date Time"
		theme_text_color: "Custom"
		md_bg_color: rgba('#C6826D')
		text_color: utils.get_color_from_hex('#F1ECEC')
        pos_hint: {'center_x': .5, 'center_y': .5}
        on_release: root.show_time_picker()

	MDFillRoundFlatButton:
		text: "Add"
		theme_text_color: 'Custom'
		md_bg_color: rgba('#C6826D')
		text_color: utils.get_color_from_hex('#F1ECEC')
		pos_hint: {'center_x': .93, 'center_y': .5}
		on_release:
			root.get_task(task_name.text, task_details.text)
			importantc.active = True

